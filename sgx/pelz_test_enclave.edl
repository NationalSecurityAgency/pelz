enclave {
	from "sgx_tsgxssl.edl" import *;
	from "sgx_pthread.edl" import *;
	include "pelz_request_handler.h"
	include "key_table.h"
	include "pelz_io.h"
	include "charbuf.h"
	include "util.h"
	include "test_helper_functions.h"
	trusted {
	public int key_table_init(void);
	public int key_table_destroy(void);
